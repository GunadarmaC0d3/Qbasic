cls
judul$ = "Program Luas Persegi Panjang"
a = LEN(judul$)
tengah = (80 / 2) - (a / 2)
PRINT STRING$(80, "-")
PRINT TAB(tengah); judul$
PRINT STRING$(80, "-")
PRINT
INPUT "Masukkan nilai panjang : ", panjang
INPUT "Masukkan nilai lebar   : ", lebar
luas = lebar * panjang
PRINT
PRINT "Luas bangunan ini      = "; luas; "m^2"
